{
  "id": null,
  "uid": "event-pipeline-dashboard",
  "title": "Event Pipeline Monitoring",
  "timezone": "browser",
  "panels": [
    {
      "type": "graph",
      "title": "Average Processing Duration (Kafka vs EventBridge)",
      "targets": [
        {
          "expr": "sum by (system, role) (rate(event_duration_seconds_sum{job=\"event_pipeline\"}[5m])) / sum by (system, role) (rate(event_duration_seconds_count{job=\"event_pipeline\"}[5m]))",
          "legendFormat": "{{system}} {{role}}"
        }
      ],
      "datasource": "Prometheus"
    },
    {
      "type": "graph",
      "title": "Total Events Processed (Kafka vs EventBridge)",
      "targets": [
        {
          "expr": "sum by (system, role) (increase(total_events{job=\"event_pipeline\"}[5m]))",
          "legendFormat": "{{system}} {{role}}"
        }
      ],
      "datasource": "Prometheus"
    },
    {
      "type": "graph",
      "title": "p95 Processing Duration (Kafka vs EventBridge)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, system, role) (rate(event_duration_seconds_bucket{job=\"event_pipeline\"}[5m])))",
          "legendFormat": "{{system}} {{role}}"
        }
      ],
      "datasource": "Prometheus"
    },
    {
      "type": "graph",
      "title": "p99 Processing Duration (Kafka vs EventBridge)",
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum by (le, system, role) (rate(event_duration_seconds_bucket{job=\"event_pipeline\"}[5m])))",
          "legendFormat": "{{system}} {{role}}"
        }
      ],
      "datasource": "Prometheus"
    }
  ],
  "schemaVersion": 26,
  "version": 1
}
